{
    "diagnostic": {
        "displayByAle": true
    },
    "coc.preferences": {
        "hoverTarget": "float"
    },
    "suggest": {
        "completionItemKindLabels": {
            "class": "\uf0e8",
            "color": "\ue22b",
            "constant": "\uf8fe",
            "default": "\uf29c",
            "enum": "\uf435",
            "enumMember": "\uf02b",
            "event": "\ufacd",
            "field": "\uf9be",
            "file": "\uf723",
            "folder": "\uf114",
            "function": "\u0192",
            "interface": "\uf417",
            "keyword": "\uf1de",
            "method": "\uf09a",
            "module": "\uf40d",
            "operator": "\u03a8",
            "property": "\ue624",
            "reference": "\ufa46",
            "snippet": "\ue60b",
            "struct": "\ufb44",
            "text": "\ue612",
            "typeParameter": "\uf728",
            "unit": "\uf475",
            "value": "\uf89f",
            "variable": "\ue79b"
        },
        "autoTrigger": "always",
        "enablePreview": true
    },
    "[php]": {
        "diagnostic.enable": false
    },
    "languageserver": {
        "kls": {
            "command": "kls",
            "args": ["--stdio"],
            "shell": true,
            "filetypes": ["kerboscript"],
            "initializationOptions": {},
            "settings": {}
        }
        // "php": {
        //     "command": "php",
        //     "args": ["-d memory_limit=1024M", "C:/wamp64/bin/serenata/bin/console", "--uri=tcp://127.0.0.1:11111"],
        //     "shell": true,
        //     "filetypes": ["php"],
        //     "initializationOptions": {},
        //     "settings": {}
        // }
    },
    "pyright": {
        "disableDiagnostics": true,
        "inlayHints.enable": false
    },
    "python": {
        "jediEnabled": false
    },
    "texlab": {
        "build": {
            "args": ["-pdf", "-interaction=nonstopmode", "-synctex=1",
                "-auxdir=log", "-outdir=build", "%f"]
        },
        "chktex": {
            "onOpenAndSave": false
        }
    }
}
